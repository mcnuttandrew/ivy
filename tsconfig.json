{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "importHelpers": true,
        "removeComments": true,
        "forceConsistentCasingInFileNames": true,
        "strict": false
    },
    "include": [
      "src/**/*",
      "untyped/index.d.ts"
    ]
}
